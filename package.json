{
  "name": "lazy-spa",
  "version": "1.0.0",
  "description": "Lazy loading SPA page based on lzyspa framework",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "autoprefixer": "^9.8.6",
    "clean-css": "^4.2.3",
    "html-minifier-terser": "^5.1.1",
    "postcss": "^7.0.32",
    "postcss-clean": "^1.1.0",
    "postcss-css-variables": "^0.17.0",
    "postcss-import": "^12.0.1",
    "prettier": "^2.0.5",
    "rollup": "^2.24.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-postcss": "^3.1.5",
    "rollup-plugin-terser": "^7.0.0",
    "sharp": "^0.25.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "build": "rm -rf public/ && rollup -c",
    "build:dev": "NODE_ENV=development npm run build",
    "deploy": "npm run build && now --prod",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "lint-fix": "tslint --fix -p tsconfig.json",
    "install:global": "npm i -g superstatic onchange",
    "serve": "NODE_ENV=development zsh serve.sh",
    "serve:prod": "zsh serve.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattiasalm/lazy.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mattiasalm/lazy/issues"
  },
  "homepage": "https://github.com/mattiasalm/lazy#readme",
  "dependencies": {
    "lzyspa": "^1.0.11"
  }
}
